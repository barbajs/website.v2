<!doctype html>
<html>

<head>
  <title>barba.js</title>
  <meta name="description" content="Barba" />

  <link rel="apple-touch-icon" sizes="180x180" href="/favicons/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/favicons/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/favicons/favicon-16x16.png">
  <link rel="manifest" href="/favicons/site.webmanifest">
  <link rel="mask-icon" href="/favicons/safari-pinned-tab.svg" color="#5bbad5">
  <link rel="shortcut icon" href="/favicons/favicon.ico">
  <meta name="msapplication-TileColor" content="#ffffff">
  <meta name="msapplication-config" content="/favicons/browserconfig.xml">
  <meta name="theme-color" content="#ffffff">

  <meta property="og:type" content="website" />
  <meta property="og:title" content="EPIC Agency - Effective creativity for digital awesomeness." />
  <meta property="og:description"
    content="EPIC is a digital agency made of passion and creativity. We craft awesome and thoughtful digital experiences that change the way you connect with clients." />
  <meta property="og:url" content="https://www.epic.net/en/" />
  <meta property="og:site_name" content="EPIC Agency" />
  <meta property="og:image" content="https://www.epic.net/wp/wp-content/uploads/2016/12/default-sharing-1024x468.jpg" />
  <meta property="og:image:secure_url"
    content="https://www.epic.net/wp/wp-content/uploads/2016/12/default-sharing-1024x468.jpg" />
  <meta property="og:image:width" content="1024" />
  <meta property="og:image:height" content="468" />

  <meta name="twitter:card" content="summary" />
  <meta name="twitter:description"
    content="EPIC is a digital agency made of passion and creativity. We craft awesome and thoughtful digital experiences that change the way you connect with clients." />
  <meta name="twitter:title" content="EPIC Agency - Effective creativity for digital awesomeness." />
  <meta name="twitter:site" content="@epicwebagency" />
  <meta name="twitter:image" content="https://www.epic.net/wp/wp-content/uploads/2016/12/default-sharing.jpg" />
  <meta name="twitter:creator" content="@epicwebagency" />

  <!-- The core Firebase JS SDK is always required and must be listed first -->
  <meta http-equiv="content-type" content="text/html; charset=utf-8" />
  <link rel="stylesheet" href="/scss/main.css" type="text/css" media="all">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <script>
    window.barbaMenu = {
      array: JSON.parse('<%-JSON.stringify(theme.menu)%>'),
    }
    window.barbaDocs = {
      array: JSON.parse('<%-JSON.stringify(theme.docs)%>'),
    }

  </script>
  <script src="/js/main.js"></script>
</head>

<body data-barba="wrapper">
  <%- partial('svg/sprite') %>
  <div data-barba="container" data-barba-namespace="<%- page.namespace %>">

    <%- partial('components/chrome/header') %>

    <main class="site-main" role="main">
      <div class="site-content">
        <div class="site-content-inner">

          <%- body %>
        </div>
      </div><!-- end .site-content -->
    </main><!-- end .site-main -->

    <%- partial('components/chrome/footer') %>

  </div>

</body>

</html>
